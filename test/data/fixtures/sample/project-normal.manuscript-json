      "title": "Experimental design of the mesocosms",
      "priority": 4,
      "createdAt": 1704391706,
      "updatedAt": 1704391706,
      "elementIDs": [
        "MPParagraphElement:E1F20882-0D69-4599-8460-BD2E08DB9796",
        "MPFigureElement:BC894179-30DD-40E3-B4AD-C6ED589B2A9A"
      ],
      "objectType": "MPSection",
      "containerID": "MPProject:828c05f2-0dc9-420a-8944-1ec9de0128c2",
      "manuscriptID": "MPManuscript:2FD119EE-42E0-4731-8574-6BB5C1C635D9"
